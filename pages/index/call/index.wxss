.getuserInfo {
  position: fixed;
  width: 80%;
  height: 30%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: #fff;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  text-align: center;
}

.getuserInfo button {
  height: 80rpx;
  line-height: 80rpx;
  width: 80%;
  background-color: #00e3a1;
  color: #fff;
}

.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  /* transform: translate(-50%,-50%); */
  background: rgba(0, 0, 0, 0.4);
}

.getuserInfo{
    background: rgba(255, 255, 255, 0.4);

}
.myTab{
  position: fixed;
  bottom: 0rpx;
  display: flex;
  justify-content: space-around;
  width: 100%;
  font-size: 22rpx;
  color: #707070;
  background-color: #fff;
}
.myTab image{
  width: 50rpx;
  height: 50rpx;
}
.myTab view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}