/* pages/address/index/index.wxss */
page{
  height: 100%;
}
.container{
  padding:0 30rpx;
height: 100%;
}
.goodsList {
  width: 100%;
  /* padding-bottom: 150rpx; */
  /* height: 100%; */
}
.goodsList .title{
  line-height: 80rpx;
  text-align: left;
  padding-left: 40rpx;
  background-color: #fff;

}
.a-gooods {
 border-radius: 15rpx;
  background-color: #fff;
  overflow: hidden;
  padding: 0 20rpx;
  border-top: 8rpx solid #f0f0f0;
  margin-bottom: 20rpx;
    box-shadow: 0px 2px 5px 0 rgba(100, 100, 100, 0.2);

}
.shopList{
  border-top: 16rpx solid #f0f0f0;
}
.shopList:first-child{
  border-top: 0px solid #f0f0f0;
}
.a-gooods-title {
  padding: 20rpx 40rpx;
  border-bottom: 1px solid #f0f0f0;
}

.a-goods-conts {
  display: flex;
  justify-content: space-between;
  /* padding-left: 30rpx; */
  /* width: 100%; */
  box-sizing: border-box;
  transition: margin-left 0.2s ease-in-out;
  background: url(https://xiaocaonuoche.com/upload/cc.png) no-repeat   left;
  background-size: 40rpx auto;

}

.a-goods-conts.active {
  background: url(data:image/gif;base64,) no-repeat  left;
  background-size: 40rpx auto;
}

.goods-num{
  height: 100rpx;
  line-height: 100rpx;
}
.goods-num text{
  margin-right: 20rpx;
}
.goods-label{
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 60rpx;
  font-size: 28rpx;
  color: #999999;
  display: flex;
}
.nolist {
  margin: 20rpx 0;
  height: 1px;
}


.text-box  {
  display:flex;
justify-content: space-between;
  border-bottom: 1px solid#eeeeee;

}
.border{
  border: none;
}
.icon-duihao1{
  font-size: 40rpx;
  line-height: 100rpx;
  height: 100rpx;
  color: #01c12d;
}
.text-box .xunze:after {
  content: "<";
  display: inline-block;
  transform: rotate(-90deg) translate(0rpx, 13rpx);
  -ms-transform: rotate(-90deg) translate(0rpx, 13rpx); /* IE 9 */
  -moz-transform: rotate(-90deg) translate(0rpx, 13rpx); /* Firefox */
  -webkit-transform: rotate(-90deg) translate(0rpx, 13rpx); /* Safari and Chrome */
  -o-transform: rotate(-90deg) translate(0rpx, 13rpx); /* Opera */
  color: #ccc;
}



.goods-info .text-box .goods-price {
  display: flex;
  color: #e64340;
  font-size: 30rpx;
  justify-content: space-between;
}


.iconfont{
  margin-right: 5rpx;
}

.cjfh{
  margin-right: 15rpx;
}
.addPhone{
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  color: #fff;
  background-color: #01c12d;
}
.green{
  color: #01c12d;
}